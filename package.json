{
  "name": "nextchat",
  "private": false,
  "license": "mit",
  "scripts": {
    "mask": "npx tsx app/masks/build.ts",
    "mask:watch": "npx watch \"yarn mask\" app/masks",
    "dev": "concurrently -r \"yarn run mask:watch\" \"next dev\"",
    "build": "yarn mask && cross-env BUILD_MODE=standalone next build",
    "start": "next start",
    "lint": "next lint",
    "export": "yarn mask && cross-env BUILD_MODE=export BUILD_APP=1 next build",
    "export:dev": "concurrently -r \"yarn mask:watch\"  \"cross-env BUILD_MODE=export BUILD_APP=1 next dev\"",
    "app:dev": "concurrently -r \"yarn mask:watch\" \"yarn tauri dev\"",
    "app:build": "yarn mask && yarn tauri build",
    "prompts": "node ./scripts/fetch-prompts.mjs",
    "prepare": "husky",
    "proxy-dev": "sh ./scripts/init-proxy.sh && proxychains -f ./scripts/proxychains.conf yarn dev",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@fortaine/fetch-event-source": "^3.0.6",
    "@hello-pangea/dnd": "^17.0.0",
    "@svgr/webpack": "^6.5.1",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "emoji-picker-react": "^4.12.0",
    "fuse.js": "^7.0.0",
    "heic2any": "^0.0.4",
    "html-to-image": "^1.11.11",
    "idb-keyval": "^6.2.1",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "markdown-to-txt": "^2.0.1",
    "mermaid": "^11.3.0",
    "nanoid": "^5.0.3",
    "next": "^14.1.1",
    "node-fetch": "^3.3.1",
    "openapi-client-axios": "^7.5.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.27.0",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "rt-client": "https://github.com/Azure-Samples/aoai-realtime-audio-sdk/releases/download/js/v0.5.0/rt-client-0.5.0.tgz",
    "sass": "^1.79.5",
    "spark-md5": "^3.0.2",
    "use-debounce": "^10.0.3",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.2",
    "@tauri-apps/cli": "2.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-katex": "^3.0.4",
    "@types/spark-md5": "^3.0.4",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "5.6.3",
    "watch": "^1.0.2",
    "webpack": "^5.95.0"
  },
  "resolutions": {
    "lint-staged/yaml": "^2.5.1"
  },
  "packageManager": "yarn@1.22.22"
}
